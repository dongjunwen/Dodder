server:
  port: 8762
spring:
  application:
    name: torrent-download-service
  redis:
    host: 127.0.0.1
    port: 6379
    jedis:
      pool:
        max-idle: 1000
        min-idle: 0
        max-active: 1000
        max-wait: -1ms

management:
  endpoints:
    web:
      exposure:
        include: ["*"]

download:
  num:
    thread: 3000
  enable-filter-sensitive-words: true     #是否开启过滤敏感词

logging:
  level:
    root: info